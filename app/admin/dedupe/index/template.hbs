`<div class="page-header">
  <h1>Dedupe</h1>
</div>

<h2>Export</h2>
<div class="text-center">
  <div class="clearfix">
    <img src="images/icon-date.svg" width="34" class="date-icon item-inblock margin-r5" />
    <div class="date-filter item-inblock">
        {{bootstrap-datepicker
            class="calendar"
            orientation="top"
            autoclose=true
            startDate=startsAt
            endDate=endsAt
            value=atDate
        }}
    </div>
  </div>
  <button class="btn btn-default" {{action "download"}}>Download</button>
</div>

<h2>Import</h2>
<div class="text-center">
  {{#if uploadProgress}}
    <div class="loader"></div>
  {{/if}}

  {{#if uploadError}}
    <div class="alert alert-danger" role="alert">
      {{uploadError}}
    </div>
  {{/if}}

  {{#if uploadSuccess}}
    <div class="alert alert-success" role="alert">
      {{uploadSuccess}}
    </div>
  {{/if}}

  {{#if showUploadButton}}
    <label class="btn btn-default">
      Upload
      {{file-uploader
        url="/api/v1.0/jobs/csv"
        method="PUT"
        onProgress="onUploadProgress"
        onSuccess="onUploadSuccess"
        onError="onUploadError"
        classNames="hidden"}}
    </label>
  {{/if}}
</div>
`
